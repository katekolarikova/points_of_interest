<h1 class="title" style="color:darkgreen;font-size:30px;text-align:center;">Users</h1>

{{#each users}}
  <div class="box box-link-hover-shadow">
    <h2 class="title">
      {{name}}
    </h2>
    <a href="/admin/dashboard/deleteUser/{{_id}}" class="button">
      <i class="fas fa-trash"></i>
    </a>
    <a href="/admin/updateuser/{{_id}}" class="button">
      <span class="icon is-small">
       <i class="fas fa-pen"></i>
      </span>
    </a>
  </div>

{{/each}}